package com.projeto.operadoresbasicos;

public class Operador {

	//somar (+)
	//subtrair (-)
	//multiplicação (*)
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int primeiro = 10;
		int segundo = 2;
		int resultado = 0;
		
		resultado = primeiro * segundo;
		System.out.println(resultado);
		

	}

}
