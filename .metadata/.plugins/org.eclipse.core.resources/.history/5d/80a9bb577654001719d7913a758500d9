package com.projeto.operadores;

public class Operador {

	//= (atribuição)
	//== (igual)
	//!= (diferente)
	//> (maior)
	//< (menor)
	//>= (maior ou igual)
	//<= (menor ou igual)
	
	//&& (e)
	//|| (ou)
	

	
	public static void main(String[] args) {
		
	boolean resultado = 0;
	int anoDeNascimento = 1986; 
	int anoCorrente = 2017;
	int idadeLimite = 18;
	int parcial;
	
	
	
	parcial = anoDeNascimento - anoCorrente;
	
	resultado = parcial >= idadeLimite;
	
	System.out.println(resultado);
		
		
	}

}
