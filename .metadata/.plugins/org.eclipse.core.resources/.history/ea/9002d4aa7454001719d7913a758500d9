public class Operador {

	//= (atribuição)
	//== (igual)
	//!= (diferente)
	//> (maior)
	//< (menor)
	//>= (maior ou igual)
	//<= (menor ou igual)
	
	//&& (e)
	

	
	public static void main(String[] args) {
		
	boolean resultado;
	int numero1=7; 
	int numero2=6;
	
	
	
	
	resultado = 1==1 && 2==2;
	

	System.out.println(resultado);
		
		
	}

